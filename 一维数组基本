#include <stdio.h>
int main()
{
	int i,a[5]={1,2,3,5,6};
	for(i=0;i<5;i++)
	printf("%d  ",a[i]); 
	/*向左平移
	 int j;
	 j=a[0];
	 printf("\n");
	 for(i=1;i<5;i++)
	  {
	  	a[i-1]=a[i];
		printf("%d  ",a[i]);
	   } 
	  a[i]=j; 
	  printf("%d",a[i]);*/
	  /*向右平移*/
	  int t;
	  t=a[4];
	  printf("\n");
	  for(i=0;i<4;i++)
	  {
	  	a[i]=a[i-1];
	    printf("%d  ",a[i]);   
	   } 
	  a[i]=t;
	  printf("%d",a[i]);
 } 
